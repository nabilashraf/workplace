{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Customer": {
    "dataSource": "db",
    "options": {
      "emailVerificationRequired": false,
      "remoting": {
        "sharedMethods": {
          "login": true,
          "logout": true,
          "create": true,
          "resetPassword": true,
          "setPassword": true,
          "changePassword": true,
          "confirm": true,
          "addRole": true,
          "prototype.__get__timeSheets": true,
          "prototype.__create__timeSheets": true
        }
      }
    }
  },
  "CustomerAccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "Email": {
    "dataSource": "email",
    "public": false
  },
  "TimeSheet": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true
        }
      }
    }
  },
  "Project": {
    "dataSource": "db",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true
        }
      }
    }
  },
  "Client": {
    "dataSource": "db",
    "options": {
      "remoting": {
        "sharedMethods": {
          "find": true,
          "create": true,
          "prototype.patchAttributes": true,
          "deleteById": true
        }
      }
    }
  },
  "Task": {
    "dataSource": "db",
    "public": true
  }
}
